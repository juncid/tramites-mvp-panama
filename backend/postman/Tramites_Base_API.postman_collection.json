              "",
              "pm.test(\"Returns second page\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.be.an('array');",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}{{api_prefix}}/tramites?skip=20&limit=20",
          "host": ["{{base_url}}{{api_prefix}}"],
          "path": ["tramites"],
          "query": [
            {
              "key": "skip",
              "value": "20"
            },
            {
              "key": "limit",
              "value": "20"
            }
          ]
        },
        "description": "Obtiene la segunda página de resultados (registros 21-40)"
      },
      "response": []
    },
    {
      "name": "Obtener Trámite por ID",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
