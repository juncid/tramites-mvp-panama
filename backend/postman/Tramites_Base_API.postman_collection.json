    {
      "name": "Crear Trámite",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 201\", function () {",
              "    pm.response.to.have.status(201);",
              "});",
              "",
              "pm.test(\"Trámite created successfully\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('id');",
              "    pm.expect(jsonData.titulo).to.exist;",
              "    pm.expect(jsonData.activo).to.eql(true);",
              "    pm.expect(jsonData.estado).to.exist;",
              "    ",
              "    // Guardar ID del nuevo trámite",
              "    pm.collectionVariables.set('tramite_id', jsonData.id);",
              "});",
              "",
              "pm.test(\"Cache invalidated\", function () {",
              "    // La creación debe invalidar el caché de listados",
              "    pm.expect(pm.response.code).to.equal(201);",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"titulo\": \"Renovación de Permiso de Trabajo\",\n  \"descripcion\": \"Trámite para renovación de permiso de trabajo temporal para extranjeros residentes en Panamá. Incluye verificación de antecedentes y estatus migratorio.\",\n  \"estado\": \"ACTIVO\",\n  \"activo\": true\n}"
        },
        "url": {
          "raw": "{{base_url}}{{api_prefix}}/tramites",
          "host": ["{{base_url}}{{api_prefix}}"],
          "path": ["tramites"]
        },
        "description": "Crea un nuevo trámite en el sistema. Automáticamente invalida el caché de listados."
      },
      "response": []
    },
